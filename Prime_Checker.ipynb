    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Input a number >> 137\n",
      "137 is prime\n"
     ]
    }
   ],
   "source": [
    "num = eval(input(\"Input a number >> \"))\n",
    "if(isprime(num) == True):\n",
    "    print(f\"{num} is prime\")\n",
    "else:\n",
    "    print(f\"{num} is not prime\")"
   ]
