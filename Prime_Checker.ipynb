   "source": [
    "def isprime(num):\n",
    "    for i in range(2, int(num/2)+1):\n",
    "        if(num % i == 0):\n",
    "            return False\n",
    "    return True"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
